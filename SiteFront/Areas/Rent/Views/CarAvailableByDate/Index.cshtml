@model CarAvailableByDateModelDto


@{
    ViewData["Title"] = " عودة السيارات";
}

<form asp-area="Rent" asp-controller="CarAvailableByDate" asp-action="Create" method="post">
    <div class="container-fluid">
        <div class="title">
            <h4>
                <span> عودة السيارات  </span>
            </h4>
        </div>
        <div class="box mb-3 p-3">
            <div class="row g-2 row-cols-1 row-cols-sm-2 row-cols-md-4 row-cols-lg-5 row-cols-xl-6">

                <div class="col">
                    <div class="form-floating mb-2">
                        <input type="date" asp-for="CarAvailableByDateRegisterDto.EndDate" class="form-control" id="" placeholder="" />
                        <label asp-for="CarAvailableByDateRegisterDto.EndDate">تاريخ العودة </label>
                        <span asp-validation-for="CarAvailableByDateRegisterDto.EndDate" class="text-danger"></span>
                    </div>
                </div>

                <div class="col">
                    <input type="submit" value="بحث" class="btn w-100 btn-primary">
                </div>
            </div>
        </div>
    </div>
</form>

<div id="_PartialCarAvailableByDate">
    <partial name="_PartialCarAvailableByDate.cshtml" model="Model.CarAvailableByDateGetDtos" />
</div>


